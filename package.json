  "private": true,
  "dependencies": {
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.56",
    "@mysteriumnetwork/terms": "0.0.27",
    "@reduxjs/toolkit": "^1.4.0",
    "lodash": "^4.17.20",
    "mysterium-vpn-js": "https://github.com/mysteriumnetwork/mysterium-vpn-js.git#d3d7edcf508e81e742bd19272b8dbede143f8735",
    "notistack": "^1.0.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-html-parser": "^2.0.2",
